{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "quotemark": [true, "single"],
        "semicolon": [true, "never"],
        "ordered-imports": [false],
        "max-classes-per-file": [false],
        "no-console": [false],
        "interface-name": [false],
        "object-literal-sort-keys": [false]
    },
    "rulesDirectory": []
}